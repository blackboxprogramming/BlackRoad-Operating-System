# 026: For Loops
# Iterate over collections

# Loop over list
fruits = ["apple", "banana", "cherry"]

for fruit in fruits:
  show "I like {fruit}"

# Loop over range
for i in 1..5:
  show "Count: {i}"

# Loop over map
user = { name: "Alex", age: 28, city: "Portland" }

for key in user.keys():
  show "{key}: {user[key]}"

# Nested loops
for x in 1..3:
  for y in 1..3:
    show "{x} Ã— {y} = {x * y}"
